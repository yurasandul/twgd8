{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "package",
            "package": {
                "name": "woocommerce/flexslider",
                "version": "2.7.2",
                "type": "drupal-library",
                "source": {
                    "url": "https://github.com/woocommerce/FlexSlider.git",
                    "type": "git",
                    "reference": "2.7.2"
                }
            }
        }
    ],
    "require": {
        "php": ">=7.1",
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6.5",
        "drupal-composer/drupal-scaffold": "^2.5",
        "drupal/address": "^1.8",
        "drupal/admin_toolbar": "^2.0",
        "drupal/backup_migrate": "^4.1",
        "drupal/captcha": "^1.1",
        "drupal/cog": "^1.15",
        "drupal/console": "^1.0.2",
        "drupal/core": "^8.7.1",
        "drupal/devel": "^2.1",
        "drupal/dropdown_language": "^2.6",
        "drupal/ds": "^3.8",
        "drupal/field_group": "^3.0",
        "drupal/flexslider": "^2.0",
        "drupal/image_widget_crop": "^2.3",
        "drupal/inline_responsive_images": "^2.1",
        "drupal/linked_field": "^1.3",
        "drupal/linkit": "^4.3",
        "drupal/menu_link_attributes": "^1.1",
        "drupal/menu_multilingual": "^1.0@alpha",
        "drupal/migrate_plus": "^4.2",
        "drupal/migrate_tools": "^4.5",
        "drupal/migrate_upgrade": "^3.1",
        "drupal/paragraphs": "^1.11",
        "drupal/pathauto": "^1.6",
        "drupal/redirect": "^1.5",
        "drupal/restui": "^1.17",
        "drupal/semanticviews": "^2.3",
        "drupal/simple_oauth": "^4.4",
        "drupal/tmgmt": "^1.11",
        "drupal/translatable_menu_link_uri": "^1.2",
        "drupal/twig_tweak": "^2.6",
        "drupal/video_embed_field": "^2.2",
        "drupal/webform": "^5.19",
        "drush/drush": "^9.0.0",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3",
        "woocommerce/flexslider": "~2.0",
        "zaporylie/composer-drupal-optimizations": "^1.0"
    },
    "require-dev": {
        "webflo/drupal-core-require-dev": "^8.7.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/{$name}": [
                "type:drupal-drush"
            ]
        },
        "drupal-scaffold": {
            "initial": {
                ".editorconfig": "../.editorconfig",
                ".gitattributes": "../.gitattributes"
            }
        },
        "patches": {
            "drupal/flexslider": {
                "Add flexslider support for any (multi-valued) entity reference field": "https://www.drupal.org/files/issues/2020-08-02/3086080-14.patch"
            },
            "drupal/core": {
                "Admin toolbar should always be rendered in the admin language (if set)": "https://www.drupal.org/files/issues/2020-08-25/2313309-67.patch"
            }
        }
    }
}
